{
  "name": "webrequest-ui",
  "version": "0.1.1",
  "description": "Webrequest UI to build awesome no-code tools",
  "scripts": {
    "build": "npm-run-all build:css",
    "build:css": "esbuild  --bundle --minify --outdir=dist/css",
    "build:sass": "sass css/webrequest-ui.scss dist/css/webrequest-ui.css",
    "build:js": "esbuild js/webrequest-ui.js --bundle --minify --outdir=dist/js",
    "watch": "npm-run-all watch:css",
    "watch:css": "esbuild css/webrequest-ui.css --bundle --watch --outdir=dist/css",
    "watch:js": "esbuild js/webrequest-ui.js --bundle --watch --outdir=dist/js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "cp node_modules/bootswatch/dist/yeti/bootstrap.css css/bootstrap.scss"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "bootswatch": "4.6.1",
    "esbuild": "^0.14.21",
    "highlight.js": "11.4.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.49.7",
    "shepherd.js": "8.3.1"
  }
}
